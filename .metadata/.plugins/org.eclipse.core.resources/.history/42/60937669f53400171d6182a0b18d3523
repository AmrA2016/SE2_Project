package com.se_project.controllers.services;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.gargoylesoftware.htmlunit.javascript.host.dom.Comment;
import com.se_project.models.Course;
import com.se_project.repositories.CommentRepository;
import com.se_project.repositories.CourseRepository;

@Service
public class CommentRepoService {
	
	
	
	@Autowired
	private CommentRepository commentRepo;
	
	public void CreateCourse(Comment comment){
		commentRepo.save(comment);
	}
	

}
